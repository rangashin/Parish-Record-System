<Page x:Class="LoginForm.Pages.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LoginForm.Pages"
      mc:Ignorable="d" 
     
      Title="Page3">

    
        <Grid Background="Azure" Height="705" VerticalAlignment="Center" Width="880">
            
            

            <DatePicker Height="25" HorizontalAlignment="Left" Margin="152,164,0,0" Name="dateDPicker1" VerticalAlignment="Top" Width="222" FontFamily="Arial" FontSize="18" />

            <!--For First Name-->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,80,0,0" Name="textBlockDFirstname" Text="First Name:" VerticalAlignment="Top" Width="110" FontFamily="Arial" FontSize="18" />
            <!--For Last Name-->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,108,0,0" Name="textBlockDLastName" Text="Last Name:" VerticalAlignment="Top" Width="110" FontFamily="Arial" FontSize="18" />
            <!--For Middle Name-->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,136,0,0" Name="textBlockDMiddleName" Text="Middle Name:" VerticalAlignment="Top" Width="110" FontFamily="Arial" FontSize="18" />
            <!--For Date of Death-->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,170,0,0" Name="textBlockDDeath" Text="Date of Death:" VerticalAlignment="Top" Width="123" FontFamily="Arial" FontSize="18"  />
            <!--For Place of Death-->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,198,0,0" Name="textBlockPDeath" Text="Place of Death:" VerticalAlignment="Top" Width="122" FontFamily="Arial" FontSize="18"  />
            <!--For Sex-->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,0,0,0" Name="textBlockDSex" Text="Sex:" VerticalAlignment="Center" Width="110" FontFamily="Arial" FontSize="18" />


            <!--For Lower Section -->
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,269,0,0" Name="textBlockDAddress" Text="Address:" VerticalAlignment="Top" Width="110" FontFamily="Arial" FontSize="18" />

            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,297,0,0" Name="textBlockDCivil" Text="Civil Status:" VerticalAlignment="Top" Width="110" FontFamily="Arial" FontSize="18" />

            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,328,0,0" Name="textBlockDCause" Text="Cause of Death:" VerticalAlignment="Top" Width="128" FontFamily="Arial" FontSize="18" />

            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,362,0,0" Name="textBlockDProvince" Text="Province:" VerticalAlignment="Top" Width="110" FontFamily="Arial" FontSize="18" />

            <TextBlock Height="23" HorizontalAlignment="Left" Margin="19,391,0,0" Name="textBlockDCity" Text="Municipality/City:" VerticalAlignment="Top" Width="139" FontFamily="Arial" FontSize="18" RenderTransformOrigin="0.179,0.263" />





            <!-- Text Boxes-->
            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,70,0,0" Name="textBoxDFirstName" VerticalAlignment="Top" Width="222" FontSize="18" />
            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,98,0,0" Name="textBoxDLastName" VerticalAlignment="Top" Width="222" FontSize="18"/>
            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,126,0,0" Name="textBoxDMiddleName" VerticalAlignment="Top" Width="222" FontSize="18"/>

            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,188,0,0" Name="textBoxDPlace" VerticalAlignment="Top" Width="222" FontSize="18" />

            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,264,0,0" Name="textBoxDAddress" VerticalAlignment="Top" Width="222" FontSize="18"/>
            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,290,0,0" Name="textBoxDCivil" VerticalAlignment="Top" Width="222" FontSize="18"/>
            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,318,0,0" Name="textBoxDCause" VerticalAlignment="Top" Width="222" FontSize="18" />

            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,346,0,0" Name="textBoxDProvince" VerticalAlignment="Top" Width="222" FontSize="18"/>

            <TextBox Height="33" HorizontalAlignment="Left" Margin="154,375,0,0" Name="textBoxDCity" VerticalAlignment="Top" Width="222" FontSize="18"/>

            <StackPanel Orientation="Vertical" Name="spRoles" Margin="152,232,439,370">
                <RadioButton Name="radMale" HorizontalAlignment="Left"  VerticalAlignment="Top" Content="Male" FontFamily="Arial" FontSize="18"/>
                <RadioButton Name="radFemale" HorizontalAlignment="Left"  VerticalAlignment="Top" Content="Female" FontFamily="Arial" FontSize="18"/>
            </StackPanel>

            <!-- Buttons -->
            <Button Content="Submit" Height="34" Width="94" HorizontalAlignment="Left" Margin="88,432,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="18"/>
            <Button Content="Clear" Height="34" Width="94" HorizontalAlignment="Left" Margin="193,432,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="18"/>
            <Button Content="Close" Height="34" HorizontalAlignment="Right" Margin="0,432,599,0" Name="button3" VerticalAlignment="Top" Width="93" FontFamily="Arial" FontSize="18" />

            <Label Content="Death Certificate" HorizontalAlignment="Left" Margin="25,15,0,0" VerticalAlignment="Top" FontSize="48" FontWeight="Bold" FontFamily="Tw Cen MT"/>
        </Grid>
    
</Page>
